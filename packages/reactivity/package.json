{
  "name": "@ld/reactivity",
  "version": "0.1.0",
  "description": "极致性能的Signal响应式系统，更新延迟<1ms",
  "type": "module",
  "main": "dist/ld-reactivity.cjs.js",
  "module": "dist/ld-reactivity.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/ld-reactivity.esm.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/ld-reactivity.cjs.js"
      }
    },
    "./signal": {
      "import": {
        "types": "./dist/types/signal.d.ts",
        "default": "./dist/signal.esm.js"
      }
    },
    "./effect": {
      "import": {
        "types": "./dist/types/effect.d.ts",
        "default": "./dist/effect.esm.js"
      }
    },
    "./computed": {
      "import": {
        "types": "./dist/types/computed.d.ts",
        "default": "./dist/computed.esm.js"
      }
    },
    "./reactive": {
      "import": {
        "types": "./dist/types/reactive.d.ts",
        "default": "./dist/reactive.esm.js"
      }
    },
    "./batch": {
      "import": {
        "types": "./dist/types/batch.d.ts",
        "default": "./dist/batch.esm.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "ld",
    "reactivity",
    "signal",
    "vue",
    "performance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fat-guy-coder/ld",
    "directory": "packages/reactivity"
  },
  "bugs": {
    "url": "https://github.com/fat-guy-coder/ld/issues"
  },
  "homepage": "https://github.com/fat-guy-coder/ld#readme",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist && tsc && rollup -c",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "bench": "node benchmarks/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run test",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@ld/build-config": "workspace:^",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.3.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}