{
  "name": "ld",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "极致性能前端框架 - performance!small!light!fast!",
  "keywords": [
    "vue3",
    "react(hooks)",
    "framework",
    "performance",
    "signals",
    "compiler",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fat-guy-coder/LD.git"
  },
  "license": "MIT",
  "author": "jason deng",
  "scripts": {
    "dev": "tsx scripts/dev.mts",
    "dev:reactivity": "pnpm --filter @ld/reactivity run dev",
    "dev:cli": "tsx scripts/cli-dev.mts",
    "dev:watch": "tsc --build --watch",
    "dev:all": "concurrently \"pnpm dev:reactivity\" \"pnpm dev:watch\" \"pnpm dev:cli\"",
    "build": "tsx scripts/build.mts",
    "build:all": "tsx scripts/build-all.mts",
    "build:prod": "tsx scripts/build-all.mts --production",
    "build:types": "tsc --build --force",
    "build:clean": "pnpm -r run clean",
    "build:fast": "tsx scripts/build-all.mts --parallel --skip-tests --skip-types",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:all": "pnpm -r run type-check",
    "type-check:watch": "tsc --noEmit --watch --project tsconfig.json",
    "tsc": "tsc",
    "test": "tsx scripts/test.mts",
    "test:watch": "tsx scripts/test.mts --watch",
    "test:coverage": "tsx scripts/test.mts --coverage",
    "test:ui": "tsx scripts/test.mts --ui",
    "test:bench": "tsx scripts/test.mts --bench",
    "bench": "tsx scripts/benchmark.mts",
    "test:ai": "tsx scripts/test-ai.mts",
    "test:ai:watch": "tsx scripts/test-ai.mts --watch",
    "test:ai:coverage": "tsx scripts/test-ai.mts --coverage",
    "test:ai:results": "tsx scripts/test-ai.mts --read-results",
    "test:ai:clean": "tsx scripts/test-ai.mts --clean",
    "test:ai:filter": "tsx scripts/test-ai.mts --filter=",
    "bench:reactivity": "tsx scripts/benchmark.mts reactivity",
    "bench:render": "tsx scripts/benchmark.mts render",
    "bench:compiler": "tsx scripts/benchmark.mts compiler",
    "bench:memory": "tsx scripts/benchmark.mts memory",
    "lint": "eslint packages/ --ext .ts,.tsx",
    "lint:fix": "eslint packages/ --ext .ts,.tsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"packages/**/*.{ts,tsx,js,json,vue}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,js,json,vue}\"",
    "check": "tsx scripts/check.mts",
    "check:quick": "tsx scripts/check.mts --skip-tests",
    "analyze": "tsx scripts/analyze.mts",
    "analyze:size": "tsx scripts/analyze.mts --size-only",
    "analyze:deps": "tsx scripts/analyze.mts --dependencies",
    "release": "tsx scripts/release.mts",
    "release:dry": "tsx scripts/release.mts --dry-run",
    "release:patch": "tsx scripts/release.mts patch",
    "release:minor": "tsx scripts/release.mts minor",
    "release:major": "tsx scripts/release.mts major",
    "publish": "pnpm run build:prod && changeset publish",
    "publish:dry": "pnpm run build:prod && changeset publish --dry-run",
    "version": "changeset version",
    "changeset": "changeset",
    "clean": "rimraf packages/*/dist packages/*/coverage packages/*/tsconfig.tsbuildinfo",
    "clean:all": "rimraf node_modules packages/*/node_modules packages/*/dist packages/*/coverage",
    "clean:reports": "rimraf reports coverage",
    "prepare": "husky install",
    "ci": "pnpm lint && pnpm type-check:all && pnpm test && pnpm build:prod",
    "ci:quick": "pnpm lint && pnpm type-check:all && pnpm build:fast"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/glob": "^9.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.11.5",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-table3": "^0.6.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^13.0.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.10",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "ora": "^6.3.1",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "semver": "^7.5.4",
    "tinybench": "^6.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^1.2.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://mirrors.cloud.tencent.com/npm/"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss,less}": [
      "prettier --write"
    ]
  }
}